{
  "name": "foodpedia",
  "version": "0.0.1",
  "description": "Foodpedia search the restaurants based on your preferences",
  "main": "index.js",
  "scripts": {
    "test": "mocha",
    "ud:dd": "serverless deploy function -f funcOne --stage dev && jovo build --stage dev && jovo deploy --stage dev --target model",
    "ud:dev": "jovo build --stage dev && jovo deploy --stage dev --target model",
    "ul": "jovo build && jovo deploy",
    "local": "jovo run -w"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "handlers/*.js": [
        "eslint --fix",
        "git add",
        "prettier --parser flow --write"
      ]
    },
    "ignore": [
      [
        "myreq.js"
      ]
    ]
  },
  "dependencies": {
    "ask-cli": "^1.5.1",
    "axios": "^0.18.0",
    "babel-preset-node6": "^11.0.0",
    "babel-preset-node8": "^1.2.0",
    "i18next-sprintf-postprocessor": "^0.2.2",
    "jovo-framework": "^1.4.16",
    "mailgun-js": "^0.22.0",
    "serverless-plugin-optimize": "^4.0.2-rc.1",
    "serverless-plugin-scripts": "^1.0.2",
    "zomato": "^1.1.0"
  },
  "devDependencies": {
    "ngrok": "^3.1.0",
    "eslint": "^5.8.0",
    "husky": "^1.1.3",
    "lint-staged": "^8.0.4",
    "prettier": "^1.14.3"
  },
  "repository": {
